# 14 reachable markings
#
# $Id$

#
#  Classic fork-join Petri net with 2 tokens.
#

PN

PLACES
  p1, p2, p3, p4, p5;

INIT
  p1 : 2;

BOUNDS
  p1 : 2,
  p2 : 2,
  p3 : 2,
  p4 : 2,
  p5 : 2;

TRANS
  t1, t2, t3, t4, t5;

ARCS
  p1 : t1, t1 : p2, t1 : p4,
  p2 : t2, t2 : p3,
  p5 : t3, t3 : p4,
  p4 : t4, t4 : p5,
  p3 : t5, p5 : t5, t5 : p1;

END
