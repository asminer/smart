# 7755605021665492992000000 States (25!/2)
#
# $Id$
#
# 5x5 tile game, the board looks like:
#
#   x11  x12  x13  x14  x15 
#
#   x21  x22  x23  x24  x25 
#
#   x31  x32  x33  x34  x35 
#
#   x41  x42  x43  x44  x45 
#
#   x51  x52  x53  x54  x55 
#
#
#   and a value of 0 means "empty space", allowing adjacent tiles to be moved.

PN
PLACES
	x11, x12, x13, x14, x15,
	x21, x22, x23, x24, x25,
	x31, x32, x33, x34, x35,
	x41, x42, x43, x44, x45,
	x51, x52, x53, x54, x55;
	
INIT
	x11 :  0, x12 :  1, x13 :  2, x14 :  3, x15 :  4,
	x21 :  5, x22 :  6, x23 :  7, x24 :  8, x25 :  9,
	x31 : 10, x32 : 11, x33 : 12, x34 : 13, x35 : 14,
	x41 : 15, x42 : 16, x43 : 17, x44 : 18, x45 : 19,
	x51 : 20, x52 : 21, x53 : 22, x54 : 23, x55 : 24;

BOUNDS
	x11 : 24, x12 : 24, x13 : 24, x14 : 24, x15 : 24,
	x21 : 24, x22 : 24, x23 : 24, x24 : 24, x25 : 24,
	x31 : 24, x32 : 24, x33 : 24, x34 : 24, x35 : 24,
	x41 : 24, x42 : 24, x43 : 24, x44 : 24, x45 : 24,
	x51 : 24, x52 : 24, x53 : 24, x54 : 24, x55 : 24;

#
# Move to the north
#
  TRANS
	n11, n12, n13, n14, n15,
	n21, n22, n23, n24, n25,
	n31, n32, n33, n34, n35,
	n41, n42, n43, n44, n45;

  ARCS
	x21 : n11 : x21,   n11 : x11 : x21,   
	x22 : n12 : x22,   n12 : x12 : x22,   
	x23 : n13 : x23,   n13 : x13 : x23,   
	x24 : n14 : x24,   n14 : x14 : x24,   
	x25 : n15 : x25,   n15 : x15 : x25,

	x31 : n21 : x31,   n21 : x21 : x31,   
	x32 : n22 : x32,   n22 : x22 : x32,   
	x33 : n23 : x33,   n23 : x23 : x33,   
	x34 : n24 : x34,   n24 : x24 : x34,   
	x35 : n25 : x35,   n25 : x25 : x35,

	x41 : n31 : x41,   n31 : x31 : x41,   
	x42 : n32 : x42,   n32 : x32 : x42,   
	x43 : n33 : x43,   n33 : x33 : x43,   
	x44 : n34 : x44,   n34 : x34 : x44,   
	x45 : n35 : x45,   n35 : x35 : x45,

	x51 : n41 : x51,   n41 : x41 : x51,   
	x52 : n42 : x52,   n42 : x42 : x52,   
	x53 : n43 : x53,   n43 : x43 : x53,   
	x54 : n44 : x54,   n44 : x44 : x54,   
	x55 : n45 : x55,   n45 : x45 : x55;

  INHIBITORS
	x11 : n11,   x12 : n12,   x13 : n13,   x14 : n14,   x15 : n15,
	x21 : n21,   x22 : n22,   x23 : n23,   x24 : n24,   x25 : n25,
	x31 : n31,   x32 : n32,   x33 : n33,   x34 : n34,   x35 : n35,
	x41 : n41,   x42 : n42,   x43 : n43,   x44 : n44,   x45 : n45;
	
#
# Move to the south
#
  TRANS
	s21, s22, s23, s24, s25,
	s31, s32, s33, s34, s35,
	s41, s42, s43, s44, s45,
	s51, s52, s53, s54, s55;

  ARCS
	x11 : s21 : x11,   s21 : x21 : x11,   
	x12 : s22 : x12,   s22 : x22 : x12,   
	x13 : s23 : x13,   s23 : x23 : x13,   
	x14 : s24 : x14,   s24 : x24 : x14,   
	x15 : s25 : x15,   s25 : x25 : x15,

	x21 : s31 : x21,   s31 : x31 : x21,   
	x22 : s32 : x22,   s32 : x32 : x22,   
	x23 : s33 : x23,   s33 : x33 : x23,   
	x24 : s34 : x24,   s34 : x34 : x24,   
	x25 : s35 : x25,   s35 : x35 : x25,

	x31 : s41 : x31,   s41 : x41 : x31,   
	x32 : s42 : x32,   s42 : x42 : x32,   
	x33 : s43 : x33,   s43 : x43 : x33,   
	x34 : s44 : x34,   s44 : x44 : x34,   
