	x35 : w34 : x35,   w34 : x34 : x35,

	x42 : w41 : x42,   w41 : x41 : x42,
	x43 : w42 : x43,   w42 : x42 : x43,
	x44 : w43 : x44,   w43 : x43 : x44,
	x45 : w44 : x45,   w44 : x44 : x45,

	x52 : w51 : x52,   w51 : x51 : x52,
	x53 : w52 : x53,   w52 : x52 : x53,
	x54 : w53 : x54,   w53 : x53 : x54,
	x55 : w54 : x55,   w54 : x54 : x55;

  INHIBITORS
	x11 : w11,   x12 : w12,   x13 : w13,   x14 : w14,
	x21 : w21,   x22 : w22,   x23 : w23,   x24 : w24,
	x31 : w31,   x32 : w32,   x33 : w33,   x34 : w34,
	x41 : w41,   x42 : w42,   x43 : w43,   x44 : w44,
	x51 : w51,   x52 : w52,   x53 : w53,   x54 : w54;

END
