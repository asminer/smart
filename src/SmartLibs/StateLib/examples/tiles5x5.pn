	x53 : e54 : x53,   e54 : x54 : x53,
	x54 : e55 : x54,   e55 : x55 : x54;

  INHIBITORS
	x12 : e12,   x13 : e13,   x14 : e14,   x15 : e15, 
	x22 : e22,   x23 : e23,   x24 : e24,   x25 : e25, 
	x32 : e32,   x33 : e33,   x34 : e34,   x35 : e35, 
	x42 : e42,   x43 : e43,   x44 : e44,   x45 : e45, 
	x52 : e52,   x53 : e53,   x54 : e54,   x55 : e55;

#
# Move to the west
#
  TRANS
	w11, w12, w13, w14, 
	w21, w22, w23, w24, 
	w31, w32, w33, w34, 
	w41, w42, w43, w44, 
	w51, w52, w53, w54;

  ARCS
	x12 : w11 : x12,   w11 : x11 : x12,
	x13 : w12 : x13,   w12 : x12 : x13,
	x14 : w13 : x14,   w13 : x13 : x14,
	x15 : w14 : x15,   w14 : x14 : x15,

